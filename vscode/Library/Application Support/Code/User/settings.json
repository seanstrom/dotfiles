{
    "editor.fontFamily": "Iosevka, 'PragmataPro', 'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "400",
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80,
        120
    ],
    "files.associations": {
        "fishfile": "ruby"
    },
    "files.insertFinalNewline": true,
    "git.allowForcePush": true,
    "git.path": "~/.local/bin/git",
    "workbench.colorTheme": "gruvboxConcoctis light soft",
    "workbench.activityBar.visible": false,
    "materialTheme.fixIconsRunning": false,
    "workbench.sideBar.location": "left",
    "prettier.eslintIntegration": true,
    "emmet.excludeLanguages": [],
    "eslint.alwaysShowStatus": true,
    "jshint.enable": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[elm]": {
        "editor.defaultFormatter": "elm-land.elm-land"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "calva.keybindingsEnabled": false,
    "vscode-neovim.neovimExecutablePaths.darwin": "~/.local/bin/nvim",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    }
}
